public class lab13 {

	public static void main(String[] args) {
		System.out.println("  _______");
		System.out.println(" /       \\");
		System.out.println("/         \\");
		System.out.println("-\"-'-\"-'-\"-");
		System.out.println("\\         /");
		System.out.println(" \\_______/");
		
	}

}
/* OUTPUT
  _______
 /       \
/         \
-"-'-"-'-"-
\         /
 \_______/       */


